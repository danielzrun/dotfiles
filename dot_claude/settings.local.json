{
  "env": {
    "MAX_MCP_OUTPUT_TOKENS": "64000",
    "BASH_DEFAULT_TIMEOUT_MS": "300000",
    "BASH_MAX_TIMEOUT_MS": "900000",
    "MAX_THINKING_TOKENS": "16384",
    "CLAUDE_CODE_MAX_OUTPUT_TOKENS": "64000",
    "CLAUDE_CODE_FILE_READ_MAX_OUTPUT_TOKENS": "45000"
  },
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(ls:*)",
      "Bash(cat:*)",
      "Bash(grep:*)",
      "Bash(rg:*)",
      "Bash(fd:*)",
      "Bash(find:*)",
      "Bash(tree:*)",
      "Bash(git status)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(git show:*)",
      "Bash(git rev-parse:*)",
      "Bash(pwd)",
      "Bash(env)",
      "Bash(true)",
      "Bash(ccusage daily:*)",
      "Bash(uv run:*)",
      "Bash(uv venv:*)",
      "Bash(uv pip install:*)",
      "Bash(pip install:*)",
      "Bash(python:*)",
      "Bash(.venv/bin/python:*)",
      "Bash(.venv/bin/pip:*)",
      "Bash(mkdir:*)",
      "Bash(cp:*)",
      "Bash(mv:*)",
      "Bash(sed:*)",
      "Bash(awk:*)",
      "Bash(chmod:*)",
      "Bash(curl:*)",
      "Bash(sh:*)",
      "Bash(uv:*)",
      "Bash(pkill:*)",
      "Bash(lsof:*)",
      "Bash(xargs:*)",
      "Bash(./start.sh:*)",
      "Bash(timeout:*)",
      "Bash(gtimeout:*)",
      "Bash(zai:*)",
      "Bash(gemini:*)",
      "Bash(mcp:*)",
      "Bash(claude config get)",
      "Bash(claude mcp:*)",
      "mcp__cf-docs__*",
      "mcp__context7__*",
      "mcp__gemini-cli__*",
      "mcp__ide__*",
      "mcp__chrome-devtools__*",
      "WebFetch(domain:docs.anthropic.com)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:openrouter.ai)",
      "WebFetch(domain:www.comet.com)",
      "WebFetch(domain:docs.convex.dev)",
      "WebFetch(domain:docs.z.ai)",
      "WebFetch(domain:pypi.org)",
      "WebSearch"
    ],
    "deny": [
      "Bash(rm -rf /:*)",
      "Bash(sudo:*)",
      "Bash(reboot)"
    ],
    "defaultMode": "plan"
  }
}
